{
  "proto": {
      "latest": "?latest",
      "date": "?date"
  },
  "$where": [
    "?_np_iri npa:hasValidSignatureForPublicKey ?pubkey .\n  ?latest npa:hasValidSignatureForPublicKey ?pubkey .\n  ?latest dct:created ?date ."
  ],
  "$orderby": [ "?date" ],
  "grlc": "#+ summary: Returns the latest nanopublication.\n#+ method: GET\n#+ pagination: 1000\n#+ endpoint_in_url: False",
  "$prefixes": {
    "np" : "http://www.nanopub.org/nschema#\nnpa" : "http://purl.org/nanopub/admin/\nnpx" : "http://purl.org/nanopub/x/\ndct" : "http://purl.org/dc/terms/"
  }
}
